{
  "name": "remark-preset-lint-travi",
  "description": "remark preset to configure remark-lint with my markdown style preferences",
  "version": "0.0.0-semantically-released",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "license": "MIT",
  "repository": "travi/remark-preset-lint-travi",
  "bugs": "https://github.com/travi/remark-preset-lint-travi/issues",
  "homepage": "https://npm.im/remark-preset-lint-travi",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "clean": "rimraf lib/",
    "lint:js": "eslint . --cache",
    "lint:md": "remark --quiet .",
    "lint:travis": "travis-lint .travis.yml",
    "lint:sensitive": "ban",
    "test": "npm-run-all --print-label --parallel lint:*",
    "build": "run-s clean build:*",
    "build:js": "rollup -c",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@travi/eslint-config-travi": "^1.6.18",
    "babel-preset-travi": "^1.4.0",
    "babel-register": "^6.26.0",
    "ban-sensitive-files": "^1.9.2",
    "commitlint-config-travi": "^1.2.30",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.1.2",
    "npm-run-all": "^4.1.3",
    "remark": "^9.0.0",
    "remark-cli": "^5.0.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.6",
    "rollup-plugin-auto-external": "^2.0.0",
    "travis-lint": "^1.0.0"
  },
  "dependencies": {
    "remark-lint-list-item-indent": "^1.0.2",
    "remark-preset-lint-recommended": "^3.0.2"
  }
}
